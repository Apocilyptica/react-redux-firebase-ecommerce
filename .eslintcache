st dispatch = useDispatch();\n\n  useEffect(() => {\n    dispatch(checkUserSession());\n  }, []);\n\n  return (\n    <div className=\"App\">\n      <AdminToolbar />\n      <Switch>\n        <Route\n          exact={true}\n          path=\"/\"\n          render={() => (\n            <HomepageLayout>\n              <Homepage />\n            </HomepageLayout>\n          )}\n        />\n        <Route\n          path=\"/registration\"\n          render={() => (\n            <MainLayout>\n              <Registration />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/login\"\n          render={() => (\n            <MainLayout>\n              <Login />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/recovery\"\n          render={() => (\n            <MainLayout>\n              <Recovery />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/dashboard\"\n          render={() => (\n            <WithAuth>\n              <DashBoardLayout>\n                <Dashboard />\n              </DashBoardLayout>\n            </WithAuth>\n          )}\n        />\n        <Route\n          path=\"/admin\"\n          render={() => (\n            <WithAdminAuth>\n              <AdminLayout>\n                <Admin />\n              </AdminLayout>\n            </WithAdminAuth>\n          )}\n        />\n      </Switch>\n    </div>\n  );\n};\n\nexport default App;\n","C:\\Users\\jvipe\\OneDrive\\Documents\\Programming School\\React Projects\\react-redux-firebase-ecommerce\\src\\components\\Header\\index.js",[],"C:\\Users\\jvipe\\OneDrive\\Documents\\Programming School\\React Projects\\react-redux-firebase-ecommerce\\src\\pages\\Homepage\\index.js",[],"C:\\Users\\jvipe\\OneDrive\\Documents\\Programming School\\React Projects\\react-redux-firebase-ecommerce\\src\\components\\Directory\\index.js",["203","204"],"import React from \"react\";\r\n\r\nimport ShopMen from \"../../assets