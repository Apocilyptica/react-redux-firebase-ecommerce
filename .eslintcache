\n  };\r\n\r\n  return (\r\n    <div className=\"admin\">\r\n      <div className=\"callToActions\">\r\n        <ul>\r\n          <li>\r\n            <Button onClick={() => toggleModal()}>Add new product</Button>\r\n          </li>\r\n        </ul>\r\n      </div>\r\n\r\n      <Modal {...configModal}>\r\n        <div className=\"addNewProductForm\">\r\n          <form onSubmit={handleSubmit}>\r\n            <h2>Add new product</h2>\r\n\r\n            <FormSelect\r\n              label=\"Category\"\r\n              options={[\r\n                {\r\n                  value: \"mens\",\r\n                  name: \"Mens\",\r\n                },\r\n                {\r\n                  value: \"womens\",\r\n                  name: \"Womens\",\r\n                },\r\n              ]}\r\n              handleChange={(e) => setProductCategory(e.target.value)}\r\n            />\r\n\r\n            <FormInput label=\"Name\" type=\"text\" value={productName} handleChange={(e) => setProductName(e.target.value)} />\r\n\r\n            <FormInput label=\"Main image URL\" type=\"url\" value={productThumbnail} handleChange={(e) => setProductThumbnail(e.target.value)} />\r\n\r\n            <FormInput\r\n              label=\"Price\"\r\n              type=\"number\"\r\n              min=\"0.00\"\r\n              max=\"10000.00\"\r\n              step=\"0.01\"\r\n              value={productPrice}\r\n              handleChange={(e) => setProductPrice(e.target.value)}\r\n            />\r\n\r\n            <CKEditor onChange={(evt) => setProductDesc(evt.editor.getData())} />\r\n            <br />\r\n\r\n            <Button type=\"submit\">Add product</Button>\r\n          </form>\r\n        </div>\r\n      </Modal>\r\n\r\n      <div className=\"manageProducts\">\r\n        <table border=\"0\" cellPadding=\"0\" cellSpacing=\"0\">\r\n          <tbody>\r\n            <tr>\r\n              <th>\r\n                <h1>Manage Products</h1>\r\n              </th>\r\n            </tr>\r\n        